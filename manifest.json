{
	"manifest_version": 2,
	"name": "Safelinks Fixer",
	"description": "Replaces Outlook Safelink URLs with a direct link",
	"version": "2.0.1",
	"homepage_url": "https://github.com/GjjvdBurg/safelinksfixer",
	"applications": {
		"gecko": {
			"id": "safelinks_fixer@gertjanvandenburg.com",
			"strict_min_version": "78.4.0"
		}
	},
	"icons": {
		"32": "icon.png",
		"64": "icon.png"
	},
	"background": {
		"scripts": [
			"scripts/background.js"
		]
	},
	"permissions": [
		"messagesModify",
		"compose"
	],
	"developer": {
		"name": "Gertjan van den Burg",
		"url": "https://gertjan.dev"
	}
}
